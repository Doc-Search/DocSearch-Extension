{
  "name": "docsearch",
  "displayName": "DocSearch",
  "description": "A tool that employs neural search to extract the most relevant excerpts from the applicable documentation and StackOverflow question answer pairs.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:docsearch.supersearch"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "docsearch.supersearch",
        "title": "Super Search"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "node-fetch": "2.0",
    "vscode": "^1.1.37"
  }
}
